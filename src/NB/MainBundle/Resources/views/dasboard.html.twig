{% extends "::base.html.twig" %}

{% block pagestyle %}
    <link href="{{ absolute_url(asset('assets/global/plugins/bootstrap-daterangepicker/daterangepicker-bs3.css')) }}" rel="stylesheet" type="text/css"/>
    <link href="{{ absolute_url(asset('assets/global/plugins/fullcalendar/fullcalendar.min.css')) }}" rel="stylesheet" type="text/css"/>
    <link href="{{ absolute_url(asset('assets/global/plugins/jqvmap/jqvmap/jqvmap.css')) }}" rel="stylesheet" type="text/css"/>
{% endblock %}

{% block pagescript %}
    <script src="{{ absolute_url(asset('assets/global/plugins/jqvmap/jqvmap/jquery.vmap.js')) }}" type="text/javascript"></script>
    <script src="{{ absolute_url(asset('assets/global/plugins/jqvmap/jqvmap/maps/jquery.vmap.russia.js')) }}" type="text/javascript"></script>
    <script src="{{ absolute_url(asset('assets/global/plugins/jqvmap/jqvmap/maps/jquery.vmap.world.js')) }}" type="text/javascript"></script>
    <script src="{{ absolute_url(asset('assets/global/plugins/jqvmap/jqvmap/maps/jquery.vmap.europe.js')) }}" type="text/javascript"></script>
    <script src="{{ absolute_url(asset('assets/global/plugins/jqvmap/jqvmap/maps/jquery.vmap.germany.js')) }}" type="text/javascript"></script>
    <script src="{{ absolute_url(asset('assets/global/plugins/jqvmap/jqvmap/maps/jquery.vmap.usa.js')) }}" type="text/javascript"></script>
    <script src="{{ absolute_url(asset('assets/global/plugins/jqvmap/jqvmap/data/jquery.vmap.sampledata.js')) }}" type="text/javascript"></script>
    <script src="{{ absolute_url(asset('assets/global/plugins/flot/jquery.flot.min.js')) }}" type="text/javascript"></script>
    <script src="{{ absolute_url(asset('assets/global/plugins/flot/jquery.flot.resize.min.js')) }}" type="text/javascript"></script>
    <script src="{{ absolute_url(asset('assets/global/plugins/flot/jquery.flot.categories.min.js')) }}" type="text/javascript"></script>
    <script src="{{ absolute_url(asset('assets/global/plugins/jquery.pulsate.min.js')) }}" type="text/javascript"></script>
    <script src="{{ absolute_url(asset('assets/global/plugins/bootstrap-daterangepicker/moment.min.js')) }}" type="text/javascript"></script>
    <script src="{{ absolute_url(asset('assets/global/plugins/bootstrap-daterangepicker/daterangepicker.js')) }}" type="text/javascript"></script>
    <!-- IMPORTANT! fullcalendar depends on jquery-ui.min.js for drag & drop support -->
    <script src="{{ absolute_url(asset('assets/global/plugins/fullcalendar/fullcalendar.min.js')) }}" type="text/javascript"></script>
    <script src="{{ absolute_url(asset('assets/global/plugins/jquery-easypiechart/jquery.easypiechart.min.js')) }}" type="text/javascript"></script>
    <script src="{{ absolute_url(asset('assets/global/plugins/jquery.sparkline.min.js')) }}" type="text/javascript"></script>
    <!-- END PAGE LEVEL PLUGINS -->
    <!-- BEGIN PAGE LEVEL SCRIPTS -->
    <script src="{{ absolute_url(asset('assets/global/scripts/metronic.js')) }}" type="text/javascript"></script>
    <script src="{{ absolute_url(asset('assets/admin/layout/scripts/layout.js')) }}" type="text/javascript"></script>
    <script src="{{ absolute_url(asset('assets/admin/layout/scripts/quick-sidebar.js')) }}" type="text/javascript"></script>
    <script src="{{ absolute_url(asset('assets/admin/layout/scripts/demo.js')) }}" type="text/javascript"></script>
    <script src="{{ absolute_url(asset('assets/admin/pages/scripts/index.js')) }}" type="text/javascript"></script>
    <script src="{{ absolute_url(asset('assets/admin/pages/scripts/tasks.js')) }}" type="text/javascript"></script>
    <!-- END PAGE LEVEL SCRIPTS -->
{% block scripts %}
    <script>
        jQuery(document).ready(function() {
            Metronic.init(); // init metronic core componets
            Layout.init(); // init layout
            QuickSidebar.init(); // init quick sidebar
            Demo.init(); // init demo features
            Index.init();
            Index.initDashboardDaterange();
            Index.initJQVMAP(); // init index page's custom scripts
            Index.initCalendar(); // init index page's custom scripts
            Index.initCharts(); // init index page's custom scripts
            Index.initChat();
            Index.initMiniCharts();
            Tasks.initDashboardWidget();
        });
    </script>

{% endblock %}


{% endblock %}
